[loggers]
keys=root,config,Copy2ObjectStorage,ObjectStore

[handlers]
keys=consoleHandler

[formatters]
keys=consoleFormatter

#----- LOGGERS

[logger_root]
level=DEBUG
handlers=consoleHandler

[logger_Copy2ObjectStorage]
level=DEBUG
handlers=consoleHandler
qualname=Copy2ObjectStorage
propagate=0

[logger_ObjectStore]
level=DEBUG
handlers=consoleHandler
qualname=ObjectStore
propagate=0

[logger_config]
level=DEBUG
handlers=consoleHandler
qualname=config
propagate=0


#----- HANDLERS

[handler_consoleHandler]
class=StreamHandler
formatter=consoleFormatter
args=(sys.stdout,)

#[handler_logFileHandler]
#class=FileHandler
#formatter=consoleFormatter
#args=('%(logfilename)s',)

#----- FORMATTERS

[formatter_consoleFormatter]
format=%(asctime)s - %(lineno)s - %(name)s - %(levelname)s - %(message)s
datefmt=%Y-%m-%d %H:%M:%S
class=logging.Formatter