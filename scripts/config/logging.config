[loggers]
keys=root,config,GetCansip,ObjectStore,GetGrib,GetGribConfig,grib2poly

[handlers]
keys=consoleHandler

[formatters]
keys=consoleFormatter

#----- LOGGERS

[logger_root]
level=INFO
handlers=consoleHandler

[logger_GetCansip]
level=INFO
handlers=consoleHandler
qualname=GetCansip
propagate=0

[logger_ObjectStore]
level=INFO
handlers=consoleHandler
qualname=ObjectStore
propagate=0

[logger_config]
level=INFO
handlers=consoleHandler
qualname=config
propagate=0

[logger_GetGrib]
level=INFO
handlers=consoleHandler
qualname=GetGrib
propagate=0

[logger_GetGribConfig]
level=INFO
handlers=consoleHandler
qualname=GetGribConfig
propagate=0

[logger_grib2poly]
level=INFO
handlers=consoleHandler
qualname=grib2poly
propagate=0

#----- HANDLERS

[handler_consoleHandler]
class=StreamHandler
formatter=consoleFormatter
args=(sys.stdout,)

#[handler_logFileHandler]
#class=FileHandler
#formatter=consoleFormatter
#args=('%(logfilename)s',)

#----- FORMATTERS

[formatter_consoleFormatter]
format=%(asctime)s - %(lineno)s - %(name)s - %(levelname)s - %(message)s
datefmt=%Y-%m-%d %H:%M:%S
class=logging.Formatter